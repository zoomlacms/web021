<script>
    $().ready(function (e) {
        $(window).scroll(function () {
            if ($(window).scrollTop() >= 50) {
                $(".index_totop").animate({ right: '0' });
            }
            else {
                $(".index_totop").animate({ right: '-71px' });
            }
        });

        $(".index_totop").click(function () {
            $("body").animate({ scrollTop: 0 }, 1000, function () {
                $(".index_totop").animate({ right: '-71px' });
            });
        });
    })
$(function () {
    $("#souchkey").val("@Request["skey"]".replace(/\|/g, " "));
})

function SearchStr() {
    var key = $("#souchkey").val().replace(" ", "");
    if (key != "") {
        var searchstr = $("#souchkey").val().trim().replace(/ +/g, '|');
        location = "postsearch?skey=" + searchstr.trim();
    }
}

var diag = new ZL_Dialog();
diag.title = "社区帮助";
diag.width = 600;
diag.content = "helpdiv";
function ShowHelp() {
    diag.ShowModal();
}

//检测固顶事件
var IO=document.getElementById('home_nav'),Y=IO,H=0,IE6;
IE6=window.ActiveXObject&&!window.XMLHttpRequest;
while(Y){H+=Y.offsetTop;Y=Y.offsetParent};
if(IE6)
    IO.style.cssText="position:absolute;top:expression(this.fix?(document"+
        ".documentElement.scrollTop-(this.javascript||"+H+")):0)";
window.onscroll=function (){
    var d=document,s=Math.max(d.documentElement.scrollTop,document.body.scrollTop);
     if(s>H&&IO.fix||s<=H&&!IO.fix)return;
    if(!IE6)IO.style.position=IO.fix?"":"fixed";        
    IO.fix=!IO.fix;
};
try{document.execCommand("BackgroundImageCache",false,true)}catch(e){};

//会员ajax登录状态需要结合JS/Controls/B_User.js引用同步生效
var buser = new B_User();
buser.IsLogged(function(data,flag){
	if(flag)
	{
		data=JSON.parse(data)
		$(".user_login").hide();
		$(".user_reg").hide();
		$(".user_cen").html("欢迎您,"+data.UserName+"!");
        $(".home_user_login>a:nth-child(1)").hide();
        $(".home_user_login p:nth-child(2)").hide();
        $(".home_user_login p:nth-child(3) a").html("欢迎您,"+data.UserName+"!");
		$(".user_cen").show();
		$(".user_logout").show();
	}
});
function LogoutFun()
{
	buser.Logout(function(){location=location;});
};

/*显示搜索框*/
