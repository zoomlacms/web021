@model ZoomLa.SQLDAL.SQL.PageSetting
@{

}
@section head{
    <title>垃圾箱</title>
}
@section content{
    <div id="pageflag" data-nav="index" data-ban="pub"></div>
    <div class="container margin_t5">
        <ol class="breadcrumb">
            <li><a title="会员中心" href="@Url.Action("Default","")">会员中心</a></li>
            <li><a href="@Url.Action("Message", "Message")">消息中心</a></li>
            <li class="active">垃圾箱</li>
        </ol>
    </div>
    <div class="container">
        @Html.Partial("MessageTop")
    </div>
    <div class="container btn_green margin_t10">
        @Html.Partial("MessageRecycle_List", Model)
        <div>
            <button id="batdel_btn" class="btn btn-primary" onclick="mvcpage.bat({ url: 'message_RealDel', confirm: '你确定要将所选中的项删除吗' });">批量删除</button>
            <button id="batrec_btn" class="btn btn-primary" onclick="mvcpage.bat({ url: 'Message_Recovery', confirm: '你确定恢复该数据吗' });">批量恢复</button>
        </div>
      
    </div>
}
